#!/bin/bash
base=$(dirname $0)
urxvt -title wmtag_1 -e $EDITOR $base/client/game.js &
urxvt -title wmtag_1 -e $EDITOR $base/client/init.js &
urxvt -title wmtag_2 -e $EDITOR $base/server/t1l.js &
urxvt -title wmtag_2 -e $EDITOR $base/server/init.js &
urxvt -title wmtag_log -e $base/node &
clear
http-server $base -p 11155
clear
